{"ast":null,"code":"import { db } from \"@/firebase/firebase.js\";\nexport default {\n  name: 'IncomeForm',\n  data() {\n    return {\n      name: '',\n      amount: null,\n      category: '',\n      date: '',\n      categories: ['Salary', 'Investments', 'Freelance', 'Other']\n    };\n  },\n  methods: {\n    submitIncome() {\n      // Add logic to submit income data to backend or store\n      console.log('Income submitted:', {\n        name: this.name,\n        amount: this.amount,\n        category: this.category,\n        date: this.date\n      });\n      // Save income to Firestore\n      db.collection('incomes').add({\n        name: this.name,\n        amount: this.amount,\n        category: this.category,\n        date: this.date\n      }).then(() => {\n        console.log('Income added to Firestore');\n      }).catch(error => {\n        console.error('Error adding income to Firestore:', error);\n      });\n\n      // Clear form fields\n      this.name = '';\n      this.amount = null;\n      this.category = '';\n      this.date = '';\n    }\n  }\n};","map":{"version":3,"names":["db","name","data","amount","category","date","categories","methods","submitIncome","console","log","collection","add","then","catch","error"],"sources":["C:\\Users\\jabri\\Desktop\\expense-tracker2\\src\\components\\IncomeForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h1>Add Income</h1>\r\n    <form>\r\n      <div class=\"form-group\">\r\n        <label for=\"name\">Name</label>\r\n        <input type=\"text\" class=\"form-control\" id=\"name\" v-model=\"name\" placeholder=\"Enter income name\">\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"amount\">Amount</label>\r\n        <input type=\"number\" class=\"form-control\" id=\"amount\" v-model=\"amount\" placeholder=\"Enter income amount\">\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"category\">Category</label>\r\n        <select class=\"form-control\" id=\"category\" v-model=\"category\">\r\n          <option v-for=\"cat in categories\" :key=\"cat\">{{cat}}</option>\r\n        </select>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"date\">Date</label>\r\n        <input type=\"date\" class=\"form-control\" id=\"date\" v-model=\"date\">\r\n      </div>\r\n      <button type=\"submit\" class=\"btn btn-primary\" @click.prevent=\"submitIncome()\">Submit</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { db } from \"@/firebase/firebase.js\";\r\n\r\nexport default {\r\n  name: 'IncomeForm',\r\n  data() {\r\n    return {\r\n      name: '',\r\n      amount: null,\r\n      category: '',\r\n      date: '',\r\n      categories: ['Salary', 'Investments', 'Freelance', 'Other']\r\n    }\r\n  },\r\n  methods: {\r\n    submitIncome() {\r\n      // Add logic to submit income data to backend or store\r\n      console.log('Income submitted:', {\r\n        name: this.name,\r\n        amount: this.amount,\r\n        category: this.category,\r\n        date: this.date\r\n      })\r\n      // Save income to Firestore\r\n      db.collection('incomes').add({\r\n        name: this.name,\r\n        amount: this.amount,\r\n        category: this.category,\r\n        date: this.date\r\n      }).then(() => {\r\n        console.log('Income added to Firestore');\r\n      }).catch((error) => {\r\n        console.error('Error adding income to Firestore:', error);\r\n      });\r\n\r\n      // Clear form fields\r\n      this.name = ''\r\n      this.amount = null\r\n      this.category = ''\r\n      this.date = ''\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n  "],"mappings":"AA4BA,SAASA,EAAC,QAAS,wBAAwB;AAE3C,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLD,IAAI,EAAE,EAAE;MACRE,MAAM,EAAE,IAAI;MACZC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,EAAE;MACRC,UAAU,EAAE,CAAC,QAAQ,EAAE,aAAa,EAAE,WAAW,EAAE,OAAO;IAC5D;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb;MAC