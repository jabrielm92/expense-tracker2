{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport firebase from \"firebase/app\";\nimport \"firebase/firestore\";\nexport default {\n  name: \"ViewIncome\",\n  data() {\n    return {\n      incomes: []\n    };\n  },\n  methods: {\n    deleteIncome(id) {\n      const index = this.incomes.findIndex(income => income.id === id);\n      const incomeRef = firebase.firestore().collection(\"incomes\").doc(id);\n      incomeRef.delete().then(() => {\n        this.incomes.splice(index, 1);\n      }).catch(error => {\n        console.error(\"Error removing income: \", error);\n      });\n    }\n  },\n  created() {\n    const userId = firebase.auth().currentUser.uid;\n    const db = firebase.firestore();\n    db.collection(\"incomes\").where(\"userId\", \"==\", userId).get().then(querySnapshot => {\n      querySnapshot.forEach(doc => {\n        this.incomes.push({\n          id: doc.id,\n          date: doc.data().date.toDate().toLocaleDateString(),\n          category: doc.data().category,\n          amount: doc.data().amount,\n          description: doc.data().description\n        });\n      });\n    }).catch(error => {\n      console.error(\"Error getting incomes: \", error);\n    });\n  }\n};","map":{"version":3,"names":["firebase","name","data","incomes","methods","deleteIncome","id","index","findIndex","income","incomeRef","firestore","collection","doc","delete","then","splice","catch","error","console","created","userId","auth","currentUser","uid","db","where","get","querySnapshot","forEach","push","date","toDate","toLocaleDateString","category","amount","description"],"sources":["C:\\Users\\jabri\\Desktop\\expense-tracker2\\src\\views\\ViewReminders.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h1>View Income</h1>\r\n    <table>\r\n      <thead>\r\n        <tr>\r\n          <th>Date</th>\r\n          <th>Category</th>\r\n          <th>Amount</th>\r\n          <th>Description</th>\r\n          <th>Edit</th>\r\n          <th>Delete</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"income in incomes\" :key=\"income.id\">\r\n          <td>{{ income.date }}</td>\r\n          <td>{{ income.category }}</td>\r\n          <td>{{ income.amount }}</td>\r\n          <td>{{ income.description }}</td>\r\n          <td>\r\n            <router-link :to=\"{ name: 'editIncome', params: { id: income.id }}\">\r\n              Edit\r\n            </router-link>\r\n          </td>\r\n          <td>\r\n            <button @click=\"deleteIncome(income.id)\">Delete</button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n<script>\r\nimport firebase from \"firebase/app\";\r\nimport \"firebase/firestore\";\r\n\r\nexport default {\r\n  name: \"ViewIncome\",\r\n  data() {\r\n    return {\r\n      incomes: [],\r\n    };\r\n  },\r\n  methods: {\r\n    deleteIncome(id) {\r\n      const index = this.incomes.findIndex((income) => income.id === id);\r\n      const incomeRef = firebase.firestore().collection(\"incomes\").doc(id);\r\n      incomeRef\r\n        .delete()\r\n        .then(() => {\r\n          this.incomes.splice(index, 1);\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Error removing income: \", error);\r\n        });\r\n    },\r\n  },\r\n  created() {\r\n    const userId = firebase.auth().currentUser.uid;\r\n    const db = firebase.firestore();\r\n    db.collection(\"incomes\")\r\n      .where(\"userId\", \"==\", userId)\r\n      .get()\r\n      .then((querySnapshot) => {\r\n        querySnapshot.forEach((doc) => {\r\n          this.incomes.push({\r\n            id: doc.id,\r\n            date: doc.data().date.toDate().toLocaleDateString(),\r\n            category: doc.data().category,\r\n            amount: doc.data().amount,\r\n            description: doc.data().description,\r\n          });\r\n        });\r\n      })\r\n      .catch((error) => {\r\n        console.error(\"Error getting incomes: \", error);\r\n      });\r\n  },\r\n};\r\n</script>\r\n<style>\r\n/* Add some styling as needed */\r\n</style>\r\n\r\n\r\n\r\n\r\n  "],"mappings":";AAkCA,OAAOA,QAAO,MAAO,cAAc;AACnC,OAAO,oBAAoB;AAE3B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAACC,EAAE,EAAE;MACf,MAAMC,KAAI,GAAI,IAAI,CAACJ,OAAO,CAACK,SAAS,CAAEC,MAAM,IAAKA,MAAM,CAACH,EAAC,KAAMA,EAAE,CAAC;MAClE,MAAMI,SAAQ,GAAIV,QAAQ,CAACW,SAAS,CAAC,CAAC,CAACC,UAAU,CAAC,SAAS,CAAC,CAACC,GAAG,CAACP,EAAE,CAAC;MACpEI,SAAQ,CACLI,MAAM,CAAC,EACPC,IAAI,CAAC,MAAM;QACV,IAAI,CAACZ,OAAO,CAACa,MAAM,CAACT,KAAK,EAAE,CAAC,CAAC;MAC/B,CAAC,EACAU,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACD,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC;MACjD,CAAC,CAAC;IACN;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR,MAAMC,MAAK,GAAIrB,QAAQ,CAACsB,IAAI,CAAC,CAAC,CAACC,WAAW,CAACC,GAAG;IAC9C,MAAMC,EAAC,GAAIzB,QAAQ,CAACW,SAAS,CAAC,CAAC;IAC/Bc,EAAE,CAACb,UAAU,CAAC,SAAS,EACpBc,KAAK,CAAC,QAAQ,EAAE,IAAI,EAAEL,MAAM,EAC5BM,GAAG,CAAC,EACJZ,IAAI,CAAEa,aAAa,IAAK;MACvBA,aAAa,CAACC,OAAO,CAAEhB,GAAG,IAAK;QAC7B,IAAI,CAACV,OAAO,CAAC2B,IAAI,CAAC;UAChBxB,EAAE,EAAEO,GAAG,CAACP,EAAE;UACVyB,IAAI,EAAElB,GAAG,CAACX,IAAI,CAAC,CAAC,CAAC6B,IAAI,CAACC,MAAM,CAAC,CAAC,CAACC,kBAAkB,CAAC,CAAC;UACnDC,QAAQ,EAAErB,GAAG,CAACX,IAAI,CAAC,CAAC,CAACgC,QAAQ;UAC7BC,MAAM,EAAEtB,GAAG,CAACX,IAAI,CAAC,CAAC,CAACiC,MAAM;UACzBC,WAAW,EAAEvB,GAAG,CAACX,IAAI,CAAC,CAAC,CAACkC;QAC1B,CAAC,CAAC;MACJ,CAAC,CAAC;IACJ,CAAC,EACAnB,KAAK,CAAEC,KAAK,IAAK;MAChBC,OAAO,CAACD,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC;IACjD,CAAC,CAAC;EACN;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}