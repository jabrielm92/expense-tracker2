{"ast":null,"code":"import { db, auth } from \"@/firebase.js\";\nexport default {\n  name: 'IncomeForm',\n  data() {\n    return {\n      name: '',\n      amount: null,\n      category: '',\n      date: '',\n      categories: ['Salary', 'Investments', 'Freelance', 'Other']\n    };\n  },\n  methods: {\n    submitExpense() {\n      // Add logic to submit expense data to backend or store\n      console.log('Income submitted:', {\n        name: this.name,\n        amount: this.amount,\n        category: this.category,\n        date: this.date\n      });\n      // Get the current user's ID\n      const userId = auth.currentUser.uid;\n      // Save income to Firestore using the user's ID as part of the collection path\n      db.collection(`users/${userId}/income`).add({\n        name: this.name,\n        amount: this.amount,\n        category: this.category,\n        date: this.date\n      }).then(docRef => {\n        console.log('Income added to Firestore');\n        // Emit an event to send the expense data to DashboardPage\n        this.$emit('income-added', {\n          id: docRef.id,\n          name: this.name,\n          amount: this.amount,\n          category: this.category,\n          date: this.date\n        });\n      }).catch(error => {\n        console.error('Error adding expense to Firestore:', error);\n      });\n\n      // Clear form fields\n      this.name = '';\n      this.amount = null;\n      this.category = '';\n      this.date = '';\n    }\n  }\n};","map":{"version":3,"names":["db","auth","name","data","amount","category","date","categories","methods","submitExpense","console","log","userId","currentUser","uid","collection","add","then","docRef","$emit","id","catch","error"],"sources":["C:\\Users\\jabri\\Desktop\\expense-tracker2\\src\\components\\IncomeForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h1>Add Income</h1>\r\n    <form>\r\n      <div class=\"form-group\">\r\n        <label for=\"name\">Name</label>\r\n        <input type=\"text\" class=\"form-control\" id=\"name\" v-model=\"name\" placeholder=\"Enter income name\">\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"amount\">Amount</label>\r\n        <input type=\"number\" class=\"form-control\" id=\"amount\" v-model=\"amount\" placeholder=\"Enter income amount\">\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"category\">Category</label>\r\n        <select class=\"form-control\" id=\"category\" v-model=\"category\">\r\n          <option v-for=\"cat in categories\" :key=\"cat\">{{cat}}</option>\r\n        </select>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"date\">Date</label>\r\n        <input type=\"date\" class=\"form-control\" id=\"date\" v-model=\"date\">\r\n      </div>\r\n      <button type=\"submit\" class=\"btn btn-primary\" @click.prevent=\"submitIncome()\">Submit</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { db, auth } from \"@/firebase.js\";\r\n\r\nexport default {\r\n  name: 'IncomeForm',\r\n  data() {\r\n    return {\r\n      name: '',\r\n      amount: null,\r\n      category: '',\r\n      date: '',\r\n      categories: ['Salary', 'Investments', 'Freelance', 'Other']\r\n    }\r\n  },\r\n  methods: {\r\n    submitExpense() {\r\n      // Add logic to submit expense data to backend or store\r\n      console.log('Income submitted:', {\r\n        name: this.name,\r\n        amount: this.amount,\r\n        category: this.category,\r\n        date: this.date\r\n      })\r\n      // Get the current user's ID\r\n      const userId = auth.currentUser.uid;\r\n      // Save income to Firestore using the user's ID as part of the collection path\r\n      db.collection(`users/${userId}/income`).add({\r\n        name: this.name,\r\n        amount: this.amount,\r\n        category: this.category,\r\n        date: this.date\r\n      }).then((docRef) => {\r\n        console.log('Income added to Firestore');\r\n        // Emit an event to send the expense data to DashboardPage\r\n        this.$emit('income-added', { id: docRef.id, name: this.name, amount: this.amount, category: this.category, date: this.date });\r\n      }).catch((error) => {\r\n        console.error('Error adding expense to Firestore:', error);\r\n      });\r\n\r\n      // Clear form fields\r\n      this.name = ''\r\n      this.amount = null\r\n      this.category = ''\r\n      this.date = ''\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AA2BA,SAASA,EAAE,EAAEC,IAAG,QAAS,eAAe;AAExC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLD,IAAI,EAAE,EAAE;MACRE,MAAM,EAAE,IAAI;MACZC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,EAAE;MACRC,UAAU,EAAE,CAAC,QAAQ,EAAE,aAAa,EAAE,WAAW,EAAE,OAAO;IAC5D;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACd;MACAC,OAAO,CAACC,GAAG,CAAC,mBAAmB,EAAE;QAC/BT,IAAI,EAAE,IAAI,CAACA,IAAI;QACfE,MAAM,EAAE,IAAI,CAACA,MAAM;QACnBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,IAAI,EAAE,IAAI,CAACA;MACb,CAAC;MACD;MACA,MAAMM,MAAK,GAAIX,IAAI,CAACY,WAAW,CAACC,GAAG;MACnC;MACAd,EAAE,CAACe,UAAU,CAAE,SAAQH,MAAO,SAAQ,CAAC,CAACI,GAAG,CAAC;QAC1Cd,IAAI,EAAE,IAAI,CAACA,IAAI;QACfE,MAAM,EAAE,IAAI,CAACA,MAAM;QACnBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,IAAI,EAAE,IAAI,CAACA;MACb,CAAC,CAAC,CAACW,IAAI,CAAEC,MAAM,IAAK;QAClBR,OAAO,CAACC,GAAG,CAAC,2BAA2B,CAAC;QACxC;QACA,IAAI,CAACQ,KAAK,CAAC,cAAc,EAAE;UAAEC,EAAE,EAAEF,MAAM,CAACE,EAAE;UAAElB,IAAI,EAAE,IAAI,CAACA,IAAI;UAAEE,MAAM,EAAE,IAAI,CAACA,MAAM;UAAEC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UAAEC,IAAI,EAAE,IAAI,CAACA;QAAK,CAAC,CAAC;MAC/H,CAAC,CAAC,CAACe,KAAK,CAAEC,KAAK,IAAK;QAClBZ,OAAO,CAACY,KAAK,CAAC,oCAAoC,EAAEA,KAAK,CAAC;MAC5D,CAAC,CAAC;;MAEF;MACA,IAAI,CAACpB,IAAG,GAAI,EAAC;MACb,IAAI,CAACE,MAAK,GAAI,IAAG;MACjB,IAAI,CAACC,QAAO,GAAI,EAAC;MACjB,IAAI,CAACC,IAAG,GAAI,EAAC;IACf;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}