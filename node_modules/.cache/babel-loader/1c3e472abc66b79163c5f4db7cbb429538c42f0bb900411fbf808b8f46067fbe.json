es: [\r\n          {\r\n            id: 1,\r\n            date: \"2022-05-01\",\r\n            category: \"Salary\",\r\n            amount: 5000,\r\n            description: \"Monthly salary\",\r\n          },\r\n          {\r\n            id: 2,\r\n            date: \"2022-05-15\",\r\n            category: \"Freelance\",\r\n            amount: 1000,\r\n            description: \"Website development\",\r\n          },\r\n        ],\r\n      };\r\n    },\r\n    methods: {\r\n      deleteIncome(id) {\r\n        const index = this.incomes.findIndex((income) => income.id === id);\r\n        this.incomes.splice(index, 1);\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  /* Add some styling as needed */\r\n  </style>\r\n  "],"mappings":"AAmCE,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,CACP;QACEC,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,YAAY;QAClBC,QAAQ,EAAE,QAAQ;QAClBC,MAAM,EAAE,IAAI;QACZC,WAAW,EAAE;MACf,CAAC,EACD;QACEJ,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,YAAY;QAClBC,QAAQ,EAAE,WAAW;QACrBC,MAAM,EAAE,IAAI;QACZC,WAAW,EAAE;MACf,CAAC;IAEL,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAACN,EAAE,EAAE;MACf,MAAMO,KAAI,GAAI,IAAI,CAACR,OAAO,CAACS,SAAS,CAAEC,MAAM,IAAKA,MAAM,CAACT,EAAC,KAAMA,EAAE,CAAC;MAClE,IAAI,CAACD,OAAO,CAACW,MAAM,CAACH,KAAK,EAAE,CAAC,CAAC;IAC/B;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}