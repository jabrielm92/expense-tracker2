{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport firebase from \"firebase\";\nexport default {\n  name: \"Income\",\n  data() {\n    return {\n      incomeEntries: []\n    };\n  },\n  methods: {\n    editIncome(id) {\n      // Add logic to edit income entry\n      console.log(\"Edit income entry with ID:\", id);\n    },\n    deleteIncome(id) {\n      // Add logic to delete income entry\n      console.log(\"Delete income entry with ID:\", id);\n    },\n    fetchIncome() {\n      const db = firebase.firestore();\n      const currentUser = firebase.auth().currentUser;\n      if (currentUser) {\n        db.collection(\"users\").doc(currentUser.uid).collection(\"income\").orderBy(\"date\", \"desc\").get().then(querySnapshot => {\n          const incomeEntries = [];\n          querySnapshot.forEach(doc => {\n            const entry = doc.data();\n            entry.id = doc.id;\n            incomeEntries.push(entry);\n          });\n          this.incomeEntries = incomeEntries;\n        }).catch(error => {\n          console.error(error);\n        });\n      }\n    }\n  },\n  created() {\n    this.fetchIncome();\n  }\n};","map":{"version":3,"names":["firebase","name","data","incomeEntries","methods","editIncome","id","console","log","deleteIncome","fetchIncome","db","firestore","currentUser","auth","collection","doc","uid","orderBy","get","then","querySnapshot","forEach","entry","push","catch","error","created"],"sources":["C:\\Users\\jabri\\Desktop\\expense-tracker2\\src\\views\\IncomeView.vue"],"sourcesContent":["<template>\r\n  <div class=\"income\">\r\n    <h1 class=\"income__title\">Income</h1>\r\n    <div class=\"income__list\">\r\n      <div v-if=\"!incomeEntries.length\" class=\"income__list__empty-message\">\r\n        No income entries to display\r\n      </div>\r\n      <div v-else>\r\n        <div v-for=\"income in incomeEntries\" :key=\"income.id\" class=\"income__list__item\">\r\n          <div class=\"income__list__item__details\">\r\n            <div class=\"income__list__item__details__name\">{{income.name}}</div>\r\n            <div class=\"income__list__item__details__amount\">+$ {{income.amount}}</div>\r\n            <div class=\"income__list__item__details__category\">{{income.category}}</div>\r\n            <div class=\"income__list__item__details__date\">{{income.date}}</div>\r\n          </div>\r\n          <div class=\"income__list__item__actions\">\r\n            <button class=\"btn btn-primary\" @click=\"editIncome(income.id)\">Edit</button>\r\n            <button class=\"btn btn-danger\" @click=\"deleteIncome(income.id)\">Delete</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport firebase from \"firebase\";\r\n\r\nexport default {\r\n  name: \"Income\",\r\n  data() {\r\n    return {\r\n      incomeEntries: [],\r\n    };\r\n  },\r\n  methods: {\r\n    editIncome(id) {\r\n      // Add logic to edit income entry\r\n      console.log(\"Edit income entry with ID:\", id);\r\n    },\r\n    deleteIncome(id) {\r\n      // Add logic to delete income entry\r\n      console.log(\"Delete income entry with ID:\", id);\r\n    },\r\n    fetchIncome() {\r\n      const db = firebase.firestore();\r\n      const currentUser = firebase.auth().currentUser;\r\n      if (currentUser) {\r\n        db.collection(\"users\")\r\n          .doc(currentUser.uid)\r\n          .collection(\"income\")\r\n          .orderBy(\"date\", \"desc\")\r\n          .get()\r\n          .then((querySnapshot) => {\r\n            const incomeEntries = [];\r\n            querySnapshot.forEach((doc) => {\r\n              const entry = doc.data();\r\n              entry.id = doc.id;\r\n              incomeEntries.push(entry);\r\n            });\r\n            this.incomeEntries = incomeEntries;\r\n          })\r\n          .catch((error) => {\r\n            console.error(error);\r\n          });\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.fetchIncome();\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n/* Add styling here */\r\n</style>\r\n\r\n\r\n  "],"mappings":";AAyBA,OAAOA,QAAO,MAAO,UAAU;AAE/B,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAACC,EAAE,EAAE;MACb;MACAC,OAAO,CAACC,GAAG,CAAC,4BAA4B,EAAEF,EAAE,CAAC;IAC/C,CAAC;IACDG,YAAYA,CAACH,EAAE,EAAE;MACf;MACAC,OAAO,CAACC,GAAG,CAAC,8BAA8B,EAAEF,EAAE,CAAC;IACjD,CAAC;IACDI,WAAWA,CAAA,EAAG;MACZ,MAAMC,EAAC,GAAIX,QAAQ,CAACY,SAAS,CAAC,CAAC;MAC/B,MAAMC,WAAU,GAAIb,QAAQ,CAACc,IAAI,CAAC,CAAC,CAACD,WAAW;MAC/C,IAAIA,WAAW,EAAE;QACfF,EAAE,CAACI,UAAU,CAAC,OAAO,EAClBC,GAAG,CAACH,WAAW,CAACI,GAAG,EACnBF,UAAU,CAAC,QAAQ,EACnBG,OAAO,CAAC,MAAM,EAAE,MAAM,EACtBC,GAAG,CAAC,EACJC,IAAI,CAAEC,aAAa,IAAK;UACvB,MAAMlB,aAAY,GAAI,EAAE;UACxBkB,aAAa,CAACC,OAAO,CAAEN,GAAG,IAAK;YAC7B,MAAMO,KAAI,GAAIP,GAAG,CAACd,IAAI,CAAC,CAAC;YACxBqB,KAAK,CAACjB,EAAC,GAAIU,GAAG,CAACV,EAAE;YACjBH,aAAa,CAACqB,IAAI,CAACD,KAAK,CAAC;UAC3B,CAAC,CAAC;UACF,IAAI,CAACpB,aAAY,GAAIA,aAAa;QACpC,CAAC,EACAsB,KAAK,CAAEC,KAAK,IAAK;UAChBnB,OAAO,CAACmB,KAAK,CAACA,KAAK,CAAC;QACtB,CAAC,CAAC;MACN;IACF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACjB,WAAW,CAAC,CAAC;EACpB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}